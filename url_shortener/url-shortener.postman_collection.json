{
  "info": {
    "name": "URL Shortener API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Create Short URL",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://localhost:3000/api/links",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["api", "links"]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n    \"url\": \"https://www.example.com\"\n}"
        }
      }
    },
    {
      "name": "Redirect to Original URL",
      "request": {
        "method": "GET",
        "url": {
          "raw": "http://localhost:3000/xnGAf0q_tbxniR-Civdce",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["xnGAf0q_tbxniR-Civdce"]
        }
      }
    },
    {
      "name": "List All Links",
      "request": {
        "method": "GET",
        "url": {
          "raw": "http://localhost:3000/api/links",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["api", "links"]
        }
      }
    }
  ]
}